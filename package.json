    "sync:stripe-db": "tsx scripts/sync-stripe-prices-pg.ts",
    "validate:stripe": "tsx scripts/validate-stripe-pg.ts",
    "validate:env": "sh scripts/validate-env.sh",
    "validate:clerk": "sh scripts/validate-clerk.sh",
    "validate:supabase": "sh scripts/validate-supabase.sh",
    "validate:upstash": "sh scripts/validate-upstash.sh",
